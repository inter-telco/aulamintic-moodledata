_nivel" locked="0"/>											<content format="xhtml"><![CDATA[<html><body bgcolor="#000000"><p align="left"><textformat leftmargin="0"><font face="Verdana" size="19" color="#FFFFFF">2</font></textformat></p></body></html>]]></content>										</text>										<image author_id="#2018121243" lang_sync="0" lang_sync_content="0" lang_sync_design="0">											<design anim_start_alpha="100" anim_start_height="30.841" anim_start_width="51.85" stoppropagation="0" vflip="0" hflip="0" rotation="0" alpha="100" proportional="1" streched="0" anim_start_top="63.319" anim_start_left="21.357" anim_length="0.3" anim_delay="1" anim_autostart="1" anim_easemode="easeOut" anim_easing="Back" anim="1" height="33.429" width="33.429" top="52.14" left="11.688" subtitle_element="0" caption="image3" locked="0"/>											<content file="@/02.png"/>										</image>									</screen>									<feedbacks>										<help nb="-1" type="undefined">										</help>										<correc if_bad="1" if_good="1" after="0" tries_nb="1">											<fb over_draw_locked="1" allow_action="0" show_bad="1" show_good="1" show_missed="0" action="none" coef="1" score="99" max_forgotten="-1">												<navigation disable_back="1" disable_next="0"/>												<screen id="page_fbv" author_id="#2018131440142">													<image id="fondo_feedback" author_id="#20181314401416" lang_sync="1" lang_sync_content="0" lang_sync_design="1">														<design locked="1"/>														<content file="IMG-feedback-correccion.png"/>													</image>													<feedback id="correccion" author_id="#20181314401417" lang_sync="1" lang_sync_content="0" lang_sync_design="1">														<design img_level_8="" img_level_7="" img_level_6="" img_level_5="" img_level_4="" img_level_3="" img_level_2="" img_level_1="" hyperlink_color_roll="#E78C07" hyperlink_color="#7AB01D" hyperlink_standard="0" selectable="0" scrollbar="0" anim_start_alpha="20" alpha="100" rotation="0" embed="1" bgcolor="#FFFFFF" tra