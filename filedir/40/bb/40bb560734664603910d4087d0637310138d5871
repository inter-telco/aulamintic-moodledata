inea_instruccion" author_id="#20181310101015" lang_sync="1" lang_sync_content="0" lang_sync_design="1">											<design locked="1"/>											<content file="LN_instruccion.png"/>										</image>										<consigne id="instruccion" author_id="#20181310101016" lang_sync="1" lang_sync_content="0" lang_sync_design="1">											<design img_level_8="" img_level_7="" img_level_6="" img_level_5="" img_level_4="" img_level_3="" img_level_2="" img_level_1="" hyperlink_color_roll="#FF0000" hyperlink_color="#0000FF" hyperlink_standard="1" selectable="0" scrollbar="0" anim_start_alpha="0" alpha="100" rotation="0" embed="0" bgcolor="#FFFFFF" transparent="1" bordercolor="#000000" border="0" fontcolor="#3C3C3B" align="left" underline="0" italic="1" bold="0" fontsize="14" font="Verdana" anim_start_top="12.445" anim_start_left="99.604" anim_length="0.5" anim_delay="1.5" anim_autostart="1" anim_easemode="easeOut" anim_easing="Regular" anim="1" height="13.614" width="49.786" top="12.445" left="99.604" subtitle_element="0" caption="" locked="0"/>											<content format="xhtml"><![CDATA[<html><body bgcolor="#FFFFFF"><p align="left"><textformat leftmargin="0"><font face="Verdana" size="14" color="#3C3C3B"><i>No olvides desplazar la barra del ejercicio hacia abajo para ver todas las opciones. Después de elegir, presiona el botón &quot;Adelante&quot; para continuar.</i></font></textformat></p></body></html>]]></content>										</consigne>										<arrow author_id="#20181310101017" lang_sync="0" lang_sync_content="0" lang_sync_design="0">											<design line_style="solid" alpha="100" transparent="0" bgcolor="#330066" border_color="#330066" border_width="3" arrow_width="10" arrow_height="10" anim_start_alpha="0" anim_length="2" anim_delay="0" anim_autostart="1" anim_easemode="easeOut" anim_easing="None" anim="0" subtitle_element="0" caption="arrow1" locked="0">																							<point1 arrow="0" y="179.45" x="417.65"/>												<point2 arrow="0" y="500" x="41