                      <elfe id="ListInText" author_id="#201814124632228" lang_sync="1" lang_sync_content="0" lang_sync_design="1">
                        <design anim_start_top="227.1" anim_start_left="57.5" anim_length="0.9" anim_delay="0.4" anim_autostart="true" anim_easemode="easeOut" anim_easing="Regular" anim="false" caption="" height="80.3165938864629" width="103.11038961039" top="29.6724890829694" left="49">
                          <text scrollbar="0" badcolor="#7F0C09" goodcolor="#648E21" bgcolor="#FFFFFF" transparent="1" bordercolor="#000000" border="0" fontcolor="#545453" align="left" underline="0" bold="0" italic="0" fontsize="16" font="Trebuchet MS"/>
