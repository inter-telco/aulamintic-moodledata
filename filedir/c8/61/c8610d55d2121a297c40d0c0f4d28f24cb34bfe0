 éxito las funciones del cargo y adaptarse a las condiciones propias de la organización. El proceso debe permitir a los candidatos conocer las características de la empresa y responsabilidades del cargo,  con el fin de que se conviertan en partícipes activos dentro del proceso, ya que el éxito de su desempeño está definido fundamentalmente por el nivel de seguridad, interés y compromiso de cada persona.","pngfb":false,"pr":{"l":"Lib","i":389}},"html5data":{"xPos":-1,"yPos":-1,"width":649,"height":347,"strokewidth":0}},"width":648,"height":346,"resume":false,"useHandCursor":true,"id":"5lmaaRd5u6F"},{"kind":"vectorshape","rotation":0,"accType":"image","cliptobounds":false,"defaultAction":"","imagelib":[{"kind":"imagedata","assetId":117,"id":"01","url":"story_content/5lJX3OqYwpV_90_DX454_DY454.swf","type":"normal","altText":"reclutamiento.png","width":435,"height":454,"mobiledx":0,"mobiledy":0}],"shapemaskId":"","xPos":176,"yPos":196,"tabIndex":242,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":109,"rotateYPos":113.5,"scaleX":100,"scaleY":100,"alpha":100,"depth":9,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":0,"top":0,"right":218,"bottom":227,"altText":"reclutamiento.png","pngfb":false,"pr":{"l":"Lib","i":391}},"html5data":{"xPos":0,"yPos":0,"width":218,"height":227,"strokewidth":0}},"width":218,"height":227,"resume":false,"useHandCursor":true,"id":"66P7ioOxdp4"},{"kind":"stategroup","objects":[{"kind":"vectorshape","rotation":0,"accType":"button","cliptobounds":false,"defaultAction":"onrelease","shapemaskId":"","xPos":0,"yPos":0,"tabIndex":237,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":13,"rotateYPos":13,"scaleX":100,"scaleY":100,"alpha":100,"depth":1,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":-1,"top":-1,"right":26,"bottom":27,"altText":"Nube de diálogo rectangular redondeada 3","pngfb":false,"pr":{"l":"Lib","i":131}},"html5data":{"xPos":-1,"yPos":-