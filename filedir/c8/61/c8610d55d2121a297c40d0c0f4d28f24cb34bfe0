ara:\\r\\n\\nCapacitar y desarrollar empleados menos productivos.\\nFacilitar el cubrimiento de vacantes con personal interno, promociones y desarrollo de personal.\\n Planificar el proceso de sucesión de talentos.\\r\\n","pngfb":false,"pr":{"l":"Lib","i":424}},"html5data":{"xPos":-1,"yPos":-1,"width":716,"height":227,"strokewidth":0}},"width":715,"height":226,"resume":false,"useHandCursor":true,"id":"5fNykzYSQRa"},{"kind":"stategroup","objects":[{"kind":"vectorshape","rotation":0,"accType":"button","cliptobounds":false,"defaultAction":"onrelease","shapemaskId":"","xPos":0,"yPos":0,"tabIndex":151,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":13,"rotateYPos":13,"scaleX":100,"scaleY":100,"alpha":100,"depth":1,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":-1,"top":-1,"right":26,"bottom":27,"altText":"Nube de diálogo rectangular redondeada 3","pngfb":false,"pr":{"l":"Lib","i":131}},"html5data":{"xPos":-1,"yPos":-1,"width":27,"height":27,"strokewidth":0}},"states":[{"kind":"state","name":"_default_Hover","data":{"hotlinkId":"","accState":0,"vectorData":{"left":-1,"top":-1,"right":26,"bottom":27,"altText":"Nube de diálogo rectangular redondeada","pngfb":false,"pr":{"l":"Lib","i":132}},"html5data":{"xPos":-1,"yPos":-1,"width":27,"height":28,"strokewidth":0}}}],"width":26,"height":26,"resume":false,"useHandCursor":true,"id":"5VNe0Pu5JN0"},{"kind":"vectorshape","rotation":0,"accType":"text","cliptobounds":false,"defaultAction":"","shapemaskId":"","xPos":5,"yPos":5,"tabIndex":152,"tabEnabled":true,"xOffset":0,"yOffset":0,"rotateXPos":8,"rotateYPos":8,"scaleX":100,"scaleY":100,"alpha":100,"depth":2,"scrolling":true,"shuffleLock":false,"data":{"hotlinkId":"","accState":0,"vectorData":{"left":0,"top":0,"right":16,"bottom":16,"altText":"Multiplicar 1","pngfb":false,"pr":{"l":"Lib","i":133}},"html5data":{"xPos":-1,"yPos":-1,"width":17,"height":17,"strokewidth":0}},"width":16,"height":16,"resume":false,"useHandCursor":true,"i