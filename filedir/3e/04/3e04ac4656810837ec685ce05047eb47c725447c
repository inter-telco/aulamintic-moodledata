97">								<metadata>																	<title><![CDATA[Pregunta]]></title>									<name><![CDATA[6]]></name>									<keywords><![CDATA[]]></keywords>								</metadata>								<navigation disable_link="0" disable_index="1"/>								<links conditional="0" default_action="" default_param="">								</links>								<page type="eval">									<metadata>																			<capacity><![CDATA[]]></capacity>										<competence><![CDATA[]]></competence>										<know_how><![CDATA[]]></know_how>									</metadata>									<navigation disable_back="1" disable_next="0" progress="1"/>									<tracking track_interaction="1" min_tries="0" binary="0" max_tries="0" coef="1" masteryscore="100" max_time="" track_score="1" track_lesson_location="1"/>									<screen id="page_multi_vrai_faux" author_id="#2018161311498">										<elfe id="MultiTrueFalse" author_id="#2018161311499" lang_sync="1" lang_sync_content="0" lang_sync_design="1">											<design anim_start_top="300" anim_start_left="457.5" anim_length="0.9" anim_delay="0.4" anim_autostart="true" anim_easemode="easeOut" anim_easing="Regular" anim="false" caption="Exercice" height="36.5502183406114" width="135.051948051948" top="51.7139737991266" left="11.6883116883117"><items missedcolor="#7A428B" badcolor="#7F0C09" goodcolor="#648E21" vspace="23" hspace="10" alternatecolor="#FFFFFF" bgcolor="#FFFFFF" transparent="1" bordercolor="#000000" border="0" fontcolor="#545453" underline="0" italic="0" bold="0" fontsize="18" font="Trebuchet MS" /><items_header fontcolor="#E95C3C" underline="0" italic="0" bold="0" fontsize="18" font="Trebuchet MS" /><items_roll fontcolor="#545453" underline="0" italic="0" bold="0" fontsize="18" font="Trebuchet MS" /><radiobutton height="38" width="38" file_missed="BTN-ejercicios-off.png" file_bad="BTN-respuesta-incorrecta.png" file_good="BTN-respuesta-correcta.png" file_uncheck_over="BTN-ejercicios-roll.png" file_uncheck_normal="BTN-ejercicios-off.png" file_check_down="BTN-eje