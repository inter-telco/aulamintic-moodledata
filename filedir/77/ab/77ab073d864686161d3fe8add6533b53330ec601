o."]}]}]},"commandset-5":{"nodeType":"g","children":[{"nodeType":"text","font-family":"ArialBold Charset1_ 61BuLP5F FS","font-size":"19px","fill":"#555555","fill-opacity":1,"children":[{"nodeType":"tspan","x":"0 12 26 37 48 59 70 81 86 94 105 116 128 140 152 163 171 176 188 199 210 222 233 238 250 262 274 286 297 302 314 325 330 341 352 358 369 374","y":18,"children":["¿Deseas reanudar desde donde dejaste "]}]},{"nodeType":"text","font-family":"ArialBold Charset1_ 61BuLP5F FS","font-size":"19px","fill":"#555555","fill-opacity":1,"children":[{"nodeType":"tspan","x":"0 5 16 21 33 38 44 49 66 77 82 93 104 114 126","y":40,"children":["la última vez?"]}]}]},"commandset-6":{"nodeType":"g","children":[{"nodeType":"text","font-family":"Arial Charset1_ 6cyRuWfzSTN FS","font-size":"16px","fill":"#353535","fill-opacity":1,"children":[{"nodeType":"tspan","x":"43 54 57","y":21,"children":["Sí"]}]}]},"commandset-7":{"nodeType":"g","children":[{"nodeType":"text","font-family":"Arial Charset1_ 6cyRuWfzSTN FS","font-size":"16px","fill":"#353535","fill-opacity":1,"children":[{"nodeType":"tspan","x":"40 51 60","y":21,"children":["No"]}]}]},"commandset-8":{"nodeType":"g","children":[{"nodeType":"g","id":"shape-1866","children":[{"nodeType":"path","id":"path-1866","fill":"#FFFFFF","fill-opacity":0,"d":"M0,0 L383,0 383,105 0,105Z"}],"className":"uneventable"},{"nodeType":"g","id":"shape-1867","children":[{"nodeType":"use","transform":[{"type":"rotate","args":[0]}],"id":"text-1868","data-reference-type":"text","data-reference-id":"01","data-shadow":false,"data-rotation":0,"x":0,"y":0}],"className":"uneventable"}]}}}');