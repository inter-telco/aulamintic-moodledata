on" author_id="#201816135167" lang_sync="1" lang_sync_content="0" lang_sync_design="1">														<design locked="1"/>														<content format="xhtml"><![CDATA[<html><body bgcolor="#000000"><p align="left"><textformat leftmargin="0"><font face="Trebuchet MS" size="18" color="#FFFFFF">Recuerda lo mencionado en el curso con lo relacionado a </font><font face="Trebuchet MS" size="18" color="#FFFFFF">las definiciones para cada tipo de gráfico en las herramientas básicas para el análisis de resultados.</font></textformat></p></body></html>]]></content>													</feedback>												</screen>											</fb>											<fb over_draw_locked="1" allow_action="0" show_bad="0" show_good="0" show_missed="0" action="none" coef="1" score="100" max_forgotten="-1">												<navigation disable_back="1" disable_next="0"/>												<screen id="page_fbv" author_id="#201816135410">													<image id="fondo_feedback" author_id="#2018161354101" lang_sync="1" lang_sync_content="0" lang_sync_design="1">														<design locked="1"/>														<content file="IMG-feedback-correccion.png"/>													</image>													<feedback id="correccion" author_id="#2018161354105" lang_sync="1" lang_sync_content="0" lang_sync_design="1">														<design img_level_8="" img_level_7="" img_level_6="" img_level_5="" img_level_4="" img_level_3="" img_level_2="" img_level_1="" hyperlink_color_roll="#E78C07" hyperlink_color="#7AB01D" hyperlink_standard="0" selectable="0" scrollbar="0" anim_start_alpha="20" alpha="100" rotation="0" embed="1" bgcolor="#FFFFFF" transparent="1" bordercolor="#FFFFFF" border="0" fontcolor="#545453" align="right" underline="0" italic="0" bold="0" fontsize="18" font="Trebuchet MS" anim_start_top="65.6" anim_start_left="84.701" anim_length="0.9" anim_delay="0.7" anim_autostart="1" anim_easemode="easeOut" anim_easing="Strong" anim="1" height="28.057" width="57.916" top="65.6" left="84.701" subtitle_element="0" caption="texto_correcci