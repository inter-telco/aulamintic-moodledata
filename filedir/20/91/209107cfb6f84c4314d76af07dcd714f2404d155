ono_importante" author_id="#20181011428114" lang_sync="1" lang_sync_content="0" lang_sync_design="1">											<design locked="1"/>											<content file="img-importante.png"/>										</image>										<image id="linea_instruccion" author_id="#20181011428115" lang_sync="1" lang_sync_content="0" lang_sync_design="1">											<design locked="1"/>											<content file="LN_instruccion.png"/>										</image>										<consigne author_id="#20181011428116" lang_sync="1" lang_sync_content="0" lang_sync_design="1">											<design img_level_8="" img_level_7="" img_level_6="" img_level_5="" img_level_4="" img_level_3="" img_level_2="" img_level_1="" hyperlink_color_roll="#E78C07" hyperlink_color="#7AB01D" hyperlink_standard="0" selectable="0" scrollbar="0" anim_start_alpha="20" alpha="100" rotation="0" embed="1" bgcolor="#FFFFFF" transparent="1" bordercolor="#000000" border="0" fontcolor="#939598" align="center" underline="0" italic="0" bold="0" fontsize="15" font="Arial Unicode MS" anim_start_top="15.786" anim_start_left="99.649" anim_length="0.5" anim_delay="1.5" anim_autostart="1" anim_easemode="easeOut" anim_easing="Regular" anim="1" height="10.83" width="49.786" top="15.786" left="99.649" subtitle_element="0" caption="instrucciones" locked="0"/>											<content format="xhtml"><![CDATA[<html><body bgcolor="#FFFFFF"><p align="left"><textformat leftmargin="0"><font face="Verdana" size="14" color="#2C2D2E"><i>Selecciona falso o verdadero, a continuación, presiona el botón &quot;Adelante&quot; para continuar.</i></font></textformat></p></body></html>]]></content>										</consigne>										<rectangle author_id="#20181011428117" lang_sync="0" lang_sync_content="0" lang_sync_design="0">											<design rounded="0" ray="1" anim_start_alpha="0" anim_start_height="0" anim_start_width="0" vflip="0" hflip="0" rotation="0" alpha="100" transparent="0" bgcolor="#E5E5E5" border="0" border_color="#333333" border_width="1" anim_start_top="0" anim_start_