a" locked="0"/>											<content format="xhtml"><![CDATA[<html><body bgcolor="#000000"><p align="left"><textformat leftmargin="0"><font face="Calibri" size="21" color="#FFFFFF"><b>Introducción a la planeación estratégica</b></font></textformat></p></body></html>]]></content>										</text>										<text author_id="#2018151427571" lang_sync="1" lang_sync_content="0" lang_sync_design="1">											<design img_level_8="" img_level_7="" img_level_6="" img_level_5="" img_level_4="" img_level_3="" img_level_2="" img_level_1="" hyperlink_color_roll="#FF0000" hyperlink_color="#0000FF" hyperlink_standard="1" selectable="0" scrollbar="0" anim_start_alpha="0" alpha="100" rotation="0" embed="0" bgcolor="#FFFFFF" transparent="1" bordercolor="#000000" border="0" fontcolor="#000000" align="left" underline="0" italic="0" bold="0" fontsize="14" font="Arial" anim_start_top="0" anim_start_left="0" anim_length="2" anim_delay="0" anim_autostart="1" anim_easemode="easeOut" anim_easing="None" anim="0" height="5.275" width="2.338" top="1.299" left="27.065" subtitle_element="0" caption="numero_nivel" locked="0"/>											<content format="xhtml"><![CDATA[<html><body bgcolor="#000000"><p align="left"><textformat leftmargin="0"><font face="Verdana" size="19" color="#FFFFFF">1</font></textformat></p></body></html>]]></content>										</text>									</screen>									<feedbacks>										<help nb="-1" type="undefined">										</help>										<correc if_bad="1" if_good="1" after="0" tries_nb="1">											<fb over_draw_locked="1" allow_action="0" show_bad="1" show_good="0" show_missed="0" action="none" coef="1" score="99" max_forgotten="-1">												<navigation disable_back="1" disable_next="0"/>												<screen id="page_fbv" author_id="#2018151427492">													<image id="fondo_feedback" author_id="#20181514274915" lang_sync="1" lang_sync_content="0" lang_sync_design="1">														<design locked="1"/>														<content file="IMG-feedback-corre